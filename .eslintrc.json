{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 1,
        "no-underscore-dangle": 0,
        "indent": [
            "error",
            2
        ],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
